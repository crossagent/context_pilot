# Google Gemini API Configuration
GOOGLE_GENAI_USE_VERTEXAI=FALSE
GOOGLE_API_KEY=you_api_key
GOOGLE_CLOUD_PROJECT=your_google_cloud_project_id
GOOGLE_CLOUD_LOCATION=us-central1

# Google Cloud Configuration
GCP_PROJECT_ID=your-project-id
GCP_REGION=us-central1

# GCS Configuration
RAG_GCS_BUCKET=your-rag-bucket-name

# Secret Manager (Optional override, default is 'pinecone-api-key')
# PINECONE_API_KEY_SECRET_ID=pinecone-api-key

# Pinecone Configuration
PINECONE_ENV=gcp-starter
PINECONE_INDEX=rag-index

# Vertex AI RAG Configuration
# Format: projects/{project}/locations/{location}/ragCorpora/{corpus_id}
RAG_CORPUS_NAME=projects/123456789/locations/us-central1/ragCorpora/1234567890